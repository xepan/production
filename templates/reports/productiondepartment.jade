.xepan-grid.main-box.clearfix.atk-table.atk-table-zebra.atk-table-bordered(id="{$_name}" class="{$class}")
    {Pannel}
    header.main-box-header.clearfix.atk-actions(class=" {$class} ")
        div.filter-block( style="100%")
            .pull-right {$grid_buttons} 
            .form-group.pull-right.xepan-filter-form
                span(class="form-control pull-left")|{$quick_search}
                    i(class="fa fa-search search-icon")
    {/Pannel}
    .main-box-body.clearfix.table
        table.table.user-list.table-hover(style="width:100%")
            {header}
            thead
                {cols}{col}
                th
                    span Department
                th
                    span Total Jobcards
                th
                    span To Received
                th
                    span Received
                th
                    span Processing
                th
                    span Forwarded
                th
                    span Completed
                th
                    span Cancelled
                th
                    span Rejected
                th 
                {/}{/}
            {/}
            {$subheader}

            tbody
                {rows}{row}
                tr(class="{$odd_even}" data-id="{$id}")
                    {cols}
                    td
                      span {$name}
                    td
                      span(class = "xepan-production-report1" data-status="all" data-id="{$id}" style= "cursor:pointer; cursor: hand; max-width:600px;"){$total_jobcards}
                    td
                      span(class = "xepan-production-report" data-status="ToReceived" data-id="{$id}" style= "cursor:pointer; cursor: hand; max-width:600px;"){$ToReceived}
                    td
                      span(class = "xepan-production-report" data-status="Received" data-id="{$id}" style= "cursor:pointer; cursor: hand; max-width:600px;"){$Received}
                    td
                      span(class = "xepan-production-report" data-status="Processing" data-id="{$id}" style= "cursor:pointer; cursor: hand; max-width:600px;"){$Processing}
                    td
                      span(class = "xepan-production-report" data-status="Forwarded" data-id="{$id}" style= "cursor:pointer; cursor: hand; max-width:600px;"){$Forwarded}
                    td
                      span(class = "xepan-production-report" data-status="Completed" data-id="{$id}" style= "cursor:pointer; cursor: hand; max-width:600px;"){$Completed}
                    td
                      span(class = "xepan-production-report" data-status="Cancelled" data-id="{$id}" style= "cursor:pointer; cursor: hand; max-width:600px;"){$Cancelled}
                    td
                      span(class = "xepan-production-report" data-status="Rejected" data-id="{$id}" style= "cursor:pointer; cursor: hand; max-width:600px;"){$Rejected}
                    {/}
                {/}{/}
                {not_found}
                div.row
                    .col-md-12.text-center.alert.alert-warning(role="alert")
                        span.icon-attention.fa.fa-exclamation-triangle  &nbsp;{$not_found_message}
                {/}

            {totals}
            tfoot
                tr(class="totals")
                    {cols}{col}
                    <td {$tdparam}>
                    {$content}
                    </td>
                    {/}{/}
            {/}
        {$Paginator}
